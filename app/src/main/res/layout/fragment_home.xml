<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="136dp"
        android:layout_height="67dp"
        android:layout_marginTop="61dp"
        android:src="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/TestCenter"
        android:layout_width="178dp"
        android:layout_height="56dp"
        android:layout_marginTop="86dp"
        android:layout_marginEnd="3dp"
        android:background="@color/colorgreen"
        android:layout_marginRight="3dp"
        android:text="Testing Centers"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/Volunteer"
        android:layout_width="171dp"
        android:layout_height="58dp"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="83dp"
        android:text="Be a Volunteer"
        android:background="@color/colorgreen"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/ContactTracing"
        android:layout_width="169dp"
        android:layout_height="62dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="98dp"
        android:layout_marginBottom="80dp"
        android:text="Contact Tracing"
        android:background="@color/colorgreen"
        app:layout_constraintBottom_toTopOf="@+id/Report"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Volunteer"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/News"
        android:layout_width="179dp"
        android:layout_height="62dp"
        android:layout_marginTop="95dp"
        android:layout_marginBottom="80dp"
        android:background="@color/colorgreen"
        android:text="News"
        app:layout_constraintBottom_toTopOf="@+id/requestServices"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TestCenter"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/Report"
        android:background="@color/colorgreen"
        android:layout_width="181dp"
        android:layout_height="66dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginBottom="156dp"
        android:text="Report Activity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/requestServices"
        android:layout_width="170dp"
        android:layout_height="76dp"
        android:layout_marginBottom="156dp"
        android:background="@color/colorgreen"
        android:text="Request Services"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="OnClick" />
</androidx.constraintlayout.widget.ConstraintLayout>