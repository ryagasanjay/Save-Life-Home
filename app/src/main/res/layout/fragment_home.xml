<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="136dp"
        android:layout_height="67dp"
        android:layout_marginTop="61dp"
        android:src="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button12"
        android:layout_width="178dp"
        android:layout_height="56dp"
        android:layout_marginTop="86dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:text="Testing Centers"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/button13"
        android:layout_width="171dp"
        android:layout_height="58dp"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="83dp"
        android:onClick="beginVoluntaryServices"
        android:text="Be a Volunteer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button14"
        android:layout_width="169dp"
        android:layout_height="62dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="98dp"
        android:layout_marginBottom="80dp"
        android:onClick="startcontactTracing"
        android:text="Contact Tracing"
        app:layout_constraintBottom_toTopOf="@+id/button16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button13"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button15"
        android:layout_width="179dp"
        android:layout_height="62dp"
        android:layout_marginTop="95dp"
        android:layout_marginBottom="80dp"
        android:onClick="showNews"
        android:text="News"
        app:layout_constraintBottom_toTopOf="@+id/button17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button16"
        android:layout_width="181dp"
        android:layout_height="66dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginBottom="156dp"
        android:text="Report Activity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button17"
        android:layout_width="170dp"
        android:layout_height="76dp"
        android:layout_marginBottom="156dp"
        android:onClick="requestEssentialServices"
        android:text="Request Services"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="OnClick" />
</androidx.constraintlayout.widget.ConstraintLayout>